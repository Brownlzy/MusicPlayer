<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="HeaderBarAddSongs">
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:background">#ffffff</item>
        <item name="android:gravity">left</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="HeaderBarEditSongs">
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:background">#ffffff</item>
        <item name="android:gravity">right</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="TabBarStyle">
        <item name="android:background">#9A46B14A</item>
    </style>

    <style name="TabBarStyleB">
        <item name="android:background">@color/green_500</item>
    </style>

    <style name="RoundedStyle">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">15dp</item>
    </style>

    <style name="CircleStyle">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="CutStyle">
        <item name="cornerFamily">cut</item>
        <item name="cornerSize">10dp</item>
    </style>

    <style name="BlurColor">
        <item name="realtimeOverlayColor">#00FFFFFF</item>
    </style>

    <style name="MainActivityBlurColor">
        <item name="realtimeOverlayColor">#4DFFFFFF</item>
    </style>

    <style name="CardViewBackgroundColor">
        <item name="cardBackgroundColor">#80FFFFFF</item>
    </style>

    <style name="DarkCardViewBackgroundColor">
        <item name="cardBackgroundColor">#CCFFFFFF</item>
    </style>

    <style name="navBackgroundColor">
        <item name="backgroundColor">#0DFFFFFF</item>
    </style>

    <style name="alertBackFrame">
        <item name="background">@android:drawable/alert_light_frame</item>>
    </style>

    <style name="SplashBackgroundColor">
        <item name="android:windowBackground">@color/white</item>
    </style>

</resources>